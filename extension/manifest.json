{
  "name": "Web Music Remote",
  "manifest_version": 2,
  "version": "0.1.0",
  "description": "Control web music players from outside the browser",
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "webRequest",
    "http://127.0.0.1:8123/*",
    "background",
    "tabs",
    "http://play.google.com/music/*",
    "https://play.google.com/music/*"
  ],
  "content_security_policy": "script-src 'self' http://127.0.0.1:8123; object-src 'self'"
}
